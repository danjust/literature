  keywords        = {Aged; Alzheimer Disease, metabolism, pathology; Amyloid beta-Peptides, metabolism; Brain, metabolism, pathology; Dendrites, metabolism, pathology; Humans; Immunohistochemistry; Microtubule-Associated Proteins, metabolism; Middle Aged},
  nlm-id          = {0412041},
