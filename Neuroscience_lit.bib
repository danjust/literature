@Article{King1998,
  author          = {King, C and Recce, M and O'Keefe, J},
  title           = {The rhythmicity of cells of the medial septum/diagonal band of Broca in the awake freely moving rat: relationships with behaviour and hippocampal theta.},
  journal         = {The European journal of neuroscience},
  year            = {1998},
  volume          = {10},
  pages           = {464--477},
  month           = feb,
  issn            = {0953-816X},
  abstract        = {Chronic extracellular recordings were obtained from cells of the medial septum and diagonal band of Broca in rats performing a simple behavioural task. The cells were found to display a variety of bursting patterns phase-locked to hippocampal theta rhythm to a greater or lesser degree. Among phase-locked cells, no systematic distribution in preferential phase could be found, and these cells were shown to maintain their preferential phase for extended periods. Cells were classified into those which showed signs of a broadening of the repolarization phase of their action potential ('inflected': putative cholinergic) and those without ('non-inflected': putative GABAergic). Non-inflected cells tended to fire rhythmic bursts while inflected cells mostly fired in an irregular fashion, although still significantly phase-locked to hippocampal theta. In neither population did the phase-locked cells show any coherent distribution of their preferential phase. Sixty-five per cent of the rhythmically bursting cells showed a significant correlation between the interburst frequency and the animal's running speed. Five cells displayed rhythmic activity only when the rat ran in a specific direction. These results have implications for models of septohippocampal function and the effects of variable septal rhythmicity on the production of hippocampal theta rhythm.},
  chemicals       = {gamma-Aminobutyric Acid},
  citation-subset = {IM},
  completed       = {1998-10-22},
  country         = {France},
  created         = {1998-10-22},
  groups          = {MSDB},
  issn-linking    = {0953-816X},
  issue           = {2},
  keywords        = {Animals; Behavior, Animal, physiology; Body Temperature, physiology; Brain, anatomy & histology, physiology; Frontal Lobe, anatomy & histology, physiology; Hippocampus, physiology; Male; Microelectrodes; Motor Activity, physiology; Parasympathetic Nervous System, physiology; Rats; Theta Rhythm; gamma-Aminobutyric Acid, physiology},
  nlm-id          = {8918110},
  owner           = {NLM},
  pmid            = {9749709},
  pubmodel        = {Print},
  pubstatus       = {ppublish},
  revised         = {2009-09-29},
}

@Article{Kropff2015,
  author          = {Kropff, Emilio and Carmichael, James E and Moser, May-Britt and Moser, Edvard I},
  title           = {Speed cells in the medial entorhinal cortex.},
  journal         = {Nature},
  year            = {2015},
  volume          = {523},
  pages           = {419--424},
  month           = jul,
  issn            = {1476-4687},
  abstract        = {Grid cells in the medial entorhinal cortex have spatial firing fields that repeat periodically in a hexagonal pattern. When animals move, activity is translated between grid cells in accordance with the animal's displacement in the environment. For this translation to occur, grid cells must have continuous access to information about instantaneous running speed. However, a powerful entorhinal speed signal has not been identified. Here we show that running speed is represented in the firing rate of a ubiquitous but functionally dedicated population of entorhinal neurons distinct from other cell populations of the local circuit, such as grid, head-direction and border cells. These 'speed cells' are characterized by a context-invariant positive, linear response to running speed, and share with grid cells a prospective bias of ∼50-80 ms. Our observations point to speed cells as a key component of the dynamic representation of self-location in the medial entorhinal cortex.},
  citation-subset = {IM},
  completed       = {2015-08-06},
  country         = {England},
  created         = {2015-07-23},
  doi             = {10.1038/nature14622},
  groups          = {Entorhinal Cortex},
  issn-linking    = {0028-0836},
  issue           = {7561},
  keywords        = {Acceleration; Action Potentials, physiology; Animals; Entorhinal Cortex, cytology, physiology; Environment; Male; Models, Neurological; Neurons, physiology; Rats; Rats, Long-Evans; Running, physiology, psychology; Time Factors},
  nlm-id          = {0410462},
  owner           = {NLM},
  pii             = {nature14622},
  pmid            = {26176924},
  pubmodel        = {Print-Electronic},
  pubstatus       = {ppublish},
  revised         = {2015-10-29},
}

@Article{Magee2000,
  author          = {Magee, J C and Cook, E P},
  title           = {Somatic EPSP amplitude is independent of synapse location in hippocampal pyramidal neurons.},
  journal         = {Nature neuroscience},
  year            = {2000},
  volume          = {3},
  pages           = {895--903},
  month           = sep,
  issn            = {1097-6256},
  abstract        = {Most neurons receive thousands of synaptic inputs onto widely spread dendrites. Because of dendritic filtering, distant synapses should have less efficacy than proximal ones. To investigate this, we characterized the amplitude and kinetics of excitatory synaptic input across the apical dendrites of CA1 pyramidal neurons using dual whole-cell recordings. We found that dendritic EPSP amplitude increases with distance from the soma, counterbalancing the filtering effects of the dendrites and reducing the location dependence of somatic EPSP amplitude. Dendritic current injections and a multi-compartmental computer model demonstrated that dendritic membrane properties have only a minor role in elevating the local EPSP. Instead a progressive increase in synaptic conductance seems to be primarily responsible for normalizing the amplitudes of individual inputs.},
  chemicals       = {Receptors, AMPA},
  citation-subset = {IM},
  completed       = {2000-09-28},
  country         = {United States},
  created         = {2000-09-28},
  doi             = {10.1038/78800},
  groups          = {Modeling},
  issn-linking    = {1097-6256},
  issue           = {9},
  keywords        = {Animals; Cell Size, physiology; Dendrites, physiology, ultrastructure; Electrophysiology; Excitatory Postsynaptic Potentials, physiology; Hippocampus, cytology, physiology; Kinetics; Models, Neurological; Pyramidal Cells, cytology, physiology; Rats; Rats, Sprague-Dawley; Receptors, AMPA, physiology; Synapses, physiology, ultrastructure; Time Factors},
  nlm-id          = {9809671},
  owner           = {NLM},
  pmid            = {10966620},
  pubmodel        = {Print},
  pubstatus       = {ppublish},
  revised         = {2007-11-14},
}

