  issue           = {1635},
  keywords        = {Action Potentials, physiology; Animals; CA1 Region, Hippocampal, cytology, physiology; Cluster Analysis; Entorhinal Cortex, cytology, physiology; Male; Pyramidal Cells, physiology; Rats; Rats, Long-Evans; Regression Analysis; Spatial Behavior, physiology; Theta Rhythm, physiology; Video Recording; entorhinal cortex; hippocampus; oscillation; space; temporal coding; theta rhythm},
  nlm             = {PMC3866450},
  nlm-id          = {7503623},
  owner           = {NLM},
  pii             = {rstb.2012.0532},
  pmc             = {PMC3866450},
