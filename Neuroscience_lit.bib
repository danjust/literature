  issue           = {8},
  keywords        = {Action Potentials, physiology; Animals; Electrodes, Implanted; Gamma Rhythm, physiology; Hippocampus, physiology; Male; Rats; Rats, Long-Evans; Running, physiology; Spectrum Analysis; Statistics as Topic; Time Factors; Wakefulness, physiology; gamma; grid cells; hippocampus; place cells; running speed},
  mid             = {NIHMS695933},
  nlm             = {PMC4499477},
  nlm-id          = {9108167},
  owner           = {NLM},
  pmc             = {PMC4499477},
  pmid            = {25601003},
