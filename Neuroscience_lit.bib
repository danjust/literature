  keywords        = {Action Potentials, physiology; Animals; CA1 Region, Hippocampal, cytology, physiology; Dendrites, physiology; Linear Models; Male; Models, Neurological; Neural Inhibition, physiology; Organ Culture Techniques; Pyramidal Cells, cytology, physiology; Rats; Rats, Wistar},
  nlm-id          = {8809320},
