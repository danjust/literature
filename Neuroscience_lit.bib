  issn-linking    = {0018-9294},
  issue           = {7},
  keywords        = {Action Potentials, physiology; Amplifiers, Electronic; Animals; Brain, physiology; Electric Stimulation, instrumentation, methods; Electrodes, Implanted; Equipment Design; Equipment Failure Analysis; Microelectrodes; Monitoring, Ambulatory, instrumentation, methods; Nerve Net, physiology; Rats; Signal Processing, Computer-Assisted, instrumentation; Systems Integration; Transistors, Electronic},
  nlm-id          = {0012737},
