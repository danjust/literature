  issn-linking    = {1050-9631},
  issue           = {6},
  keywords        = {Animals; Female; Hippocampus, cytology, physiology; Movement, physiology; Neurons, physiology; Orientation, physiology; Rats; Space Perception, physiology; Video Recording},
  nlm-id          = {9108167},
