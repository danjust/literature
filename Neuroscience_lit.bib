  groups          = {Entorhinal Cortex},
  issn-linking    = {1050-9631},
  issue           = {5},
  keywords        = {Animals; Axons, ultrastructure; Dendrites, ultrastructure; Electrophysiology; Entorhinal Cortex, cytology, physiology, ultrastructure; In Vitro Techniques; Lysine, analogs & derivatives; Male; Neural Pathways, anatomy & histology, cytology, physiology; Neurons, physiology, ultrastructure; Rats; Rats, Wistar},
