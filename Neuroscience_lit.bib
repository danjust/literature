  pmid            = {27526205},
  pubmodel        = {Print-Electronic},
  pubstatus       = {ppublish},
  revised         = {2016-09-27},
}

@Article{Fuchs2016,
  author          = {Fuchs, Elke C and Neitz, Angela and Pinna, Roberta and Melzer, Sarah and Caputi, Antonio and Monyer, Hannah},
  title           = {Local and Distant Input Controlling Excitation in Layer II of the Medial Entorhinal Cortex.},
  journal         = {Neuron},
  year            = {2016},
  volume          = {89},
  pages           = {194--208},
  month           = jan,
  issn            = {1097-4199},
  abstract        = {Layer II (LII) of the medial entorhinal cortex (MEC) comprises grid cells that support spatial navigation. The firing pattern of grid cells might be explained by attractor dynamics in a network, which requires either direct excitatory connectivity between phase-specific grid cells or indirect coupling via interneurons. However, knowledge regarding local networks that support in vivo activity is incomplete. Here we identified essential components of LII networks in the MEC. We distinguished four types of excitatory neurons that exhibit cell-type-specific local excitatory and inhibitory connectivity. Furthermore, we found that LII neurons contribute to the excitation of contralateral neurons in the corresponding layer. Finally, we demonstrated that the medial septum controls excitation in the MEC via two subpopulations of long-range GABAergic neurons that target distinct interneurons in LII, thereby disinhibiting local circuits. We thus identified local connections that could support attractor dynamics and external inputs that likely govern excitation in LII.},
