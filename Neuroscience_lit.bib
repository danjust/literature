  doi             = {10.1002/hipo.20113},
  issn-linking    = {1050-9631},
  issue           = {7},
  keywords        = {Animals; Biological Clocks, physiology; Hippocampus, physiology; Humans; Memory, physiology; Neural Pathways, physiology; Neuronal Plasticity, physiology; Orientation, physiology; Space Perception, physiology; Synaptic Transmission, physiology; Theta Rhythm},
  nlm-id          = {9108167},
