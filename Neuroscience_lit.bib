  issue           = {2},
  keywords        = {Animals; Calcium Channels, metabolism; Dendrites, physiology; Excitatory Postsynaptic Potentials, physiology; Glutamic Acid, metabolism; Patch-Clamp Techniques; Pyramidal Cells, physiology; Rats; Rats, Sprague-Dawley; Receptors, N-Methyl-D-Aspartate, metabolism; Synapses, physiology; Synaptic Transmission, physiology},
  nlm-id          = {8809320},
