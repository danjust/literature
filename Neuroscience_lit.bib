  issue           = {4},
  keywords        = {Animals; Cerebral Cortex, anatomy & histology, physiology; Hippocampus, anatomy & histology, physiology; Neural Pathways, anatomy & histology, physiology; Parahippocampal Gyrus, anatomy & histology, physiology},
  nlm-id          = {9108167},
