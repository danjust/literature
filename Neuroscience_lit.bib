  issue           = {7},
  keywords        = {Animals; CA1 Region, Hippocampal, cytology, growth & development; Entorhinal Cortex, cytology, growth & development; Male; Neural Pathways, cytology, growth & development; Neurons, classification, physiology; Pyramidal Cells, physiology; Rats; entorhinal cortex; hippocampus; phase precession; pyramidal cell; stellate cell; temporal code},
  nlm-id          = {8102140},
