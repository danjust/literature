  groups          = {MSDB},
  issn-linking    = {0028-0836},
  issue           = {6195},
  keywords        = {Afferent Pathways, physiology; Animals; Hippocampus, cytology, physiology; Interneurons, physiology; Neural Inhibition; Neurons, Afferent, cytology, physiology; Rats; Septum Pellucidum, cytology, physiology; gamma-Aminobutyric Acid, physiology},
  nlm-id          = {0410462},
