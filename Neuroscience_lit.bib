  keywords        = {Animals; Corpus Striatum, physiology; Electric Stimulation; Evoked Potentials; Guinea Pigs; Hippocampus, physiology; In Vitro Techniques; Male; Neurons, physiology; Pyramidal Tracts, physiology; Synapses, physiology},
  nlm-id          = {0045503},
